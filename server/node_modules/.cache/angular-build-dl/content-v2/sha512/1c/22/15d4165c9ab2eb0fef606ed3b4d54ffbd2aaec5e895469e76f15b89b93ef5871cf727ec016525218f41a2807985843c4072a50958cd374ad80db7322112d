let WebsiteService = class WebsiteService {
    constructor(http, tokenExtractor) {
        this.http = http;
        this.tokenExtractor = tokenExtractor;
        this.baseUrl = _environments_environment__WEBPACK_IMPORTED_MODULE_3__["environment"].baseUrl;
    }
    getAllBlogs() {
        return this.http.get(`${this.baseUrl}/get-blogs`);
    }
    getBlogById(id) {
        return this.http.get(`${this.baseUrl}/get-blog/${id}`);
    }
    // Authentications Calls
    register(user) {
        return this.http.post(`${this.baseUrl}/register`, user);
    }
    login(user) {
        let csrfToken = this.tokenExtractor.getToken();
        console.log(csrfToken);
        let headers = new _angular_common_http__WEBPACK_IMPORTED_MODULE_1__["HttpHeaders"]();
        headers = headers.set('X-XSRF-TOKEN', csrfToken);
        return this.http.post(`${this.baseUrl}/login`, user, { headers });
    }
    getUSer() {
        let token = localStorage.getItem("userToken");
        let headers = new _angular_common_http__WEBPACK_IMPORTED_MODULE_1__["HttpHeaders"]();
        headers = headers.set('Authorization', `Bearer ${token}`);
        return this.http.get(`${this.baseUrl}/user`, { headers });
    }
    // Authentications Calls End
    getAllServices(type) {
        return this.http.get(`${this.baseUrl}/get-services/${type}`);
    }
    getAllPackages(type) {
        return this.http.get(`${this.baseUrl}/get-packages/${type}`);
    }
    createOrder(payment) {
        let token = localStorage.getItem("userToken");
        let headers = new _angular_common_http__WEBPACK_IMPORTED_MODULE_1__["HttpHeaders"]();
        headers = headers.set('Authorization', `Bearer ${token}`);
        return this.http.post(`${this.baseUrl}/create-order`, payment, { headers });
    }
    createCritique(critique) {
        let token = localStorage.getItem("userToken");
        let headers = new _angular_common_http__WEBPACK_IMPORTED_MODULE_1__["HttpHeaders"]();
        headers = headers.set('Authorization', `Bearer ${token}`);
        return this.http.post(`${this.baseUrl}/create-critique`, critique, { headers });
    }
};
