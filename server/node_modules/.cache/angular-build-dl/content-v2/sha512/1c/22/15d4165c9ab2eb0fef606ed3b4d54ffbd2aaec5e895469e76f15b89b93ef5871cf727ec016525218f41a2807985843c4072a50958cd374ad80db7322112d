let ResumeServicesComponent = class ResumeServicesComponent {
    constructor(modalService, webService, router) {
        this.modalService = modalService;
        this.webService = webService;
        this.router = router;
        this.services = [];
        this.packages = [];
        this.navbarClass = "navbar2";
        this.amount = null;
    }
    buy(amount, id, type) {
        if (localStorage.getItem("userToken")) {
            const initialState = {
                amount: amount,
                id: id,
                service_type_id: 1,
                type: type
            };
            this.bsModalRef = this.modalService.show(_shared_components_stripe_stripe_component__WEBPACK_IMPORTED_MODULE_7__["StripeComponent"], { class: 'modal-dialog-centered', initialState });
            this.bsModalRef.content.closeBtnName = 'Close';
        }
        else {
            this.bsModalRef = this.modalService.show(_shared_components_login_check_modal_login_check_modal_component__WEBPACK_IMPORTED_MODULE_6__["LoginCheckModalComponent"], { class: 'modal-dialog-centered' });
            this.bsModalRef.content.closeBtnName = 'Close';
        }
    }
    ngOnInit() {
        this.getPackages();
        this.getServices();
    }
    getServices() {
        this.webService.getAllServices("Private Sector").subscribe((res) => {
            console.log(res);
            this.services = res.service[0].services;
        }, (error) => {
            console.log(error);
        });
    }
    getPackages() {
        this.webService.getAllPackages("Private Sector").subscribe((res) => {
            console.log(res);
            this.packages = res.packages;
        }, (error) => {
            console.log(error);
        });
    }
};
